    <script src="localhost::"></script>
</head>

<body>
    {{>navbar}}
    {{>logIn}}
    <div class="container pt-3">
        <div class="row">
            <div class="col">
                <form action="/contact" method="POST">
                    <div class="mb-3">
                        <label for="exampleFormControlInput1" class="form-label">Name</label>
                        <input type="text" class="form-control" id="name" placeholder="Enter your fullName" name="name">
                    </div>
                    <div class="mb-3">
                        <label for="exampleFormControlInput1" class="form-label">Email address</label>
                        <input type="email" class="form-control" id="email" placeholder="name@example.com" name="email">
                    </div>
                    <div class="mb-3">
                        <label for="exampleFormControlInput1" class="form-label">Moble No.</label>
                        <input type="text" class="form-control" id="mobile" placeholder="+91 7564873856" name="mobile">
                    </div>
                    <div class="mb-3">
                        <label for="exampleFormControlTextarea1" class="form-label">Enter Your Quiery</label>
                        <input type="text" class="form-control" id="message" placeholder="This is Awesome"
                            name="message">
                    </div>
                    <div class="mb-0">
                        <button type="submit" class="btn btn-primary">Send</button>
                    </div>
                </form>
            </div>
            <div class="col offset-md-2">
                <img src="media/indicate11.png" alt="Error">
            </div>
        </div>
    </div>
    {{>join}}
    {{>footer}}


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>
    <script src="js/btn.js"></script>
    <script>
        var typed1 = new Typed(".multiText1", {
            strings: ["#twitter", "#Instagram", "#LinkedIn"],
            loop: true,
            typeSpeed: 100,
            backSpeed: 80
        })
    </script>
</body>

</html>